{
  "name": "navarr/yii2-polymer-material",
  "description": "Material Design library using Polymer for Yii2 Framework",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Navarr Barnier",
      "email": "me@navarr.me"
    }
  ],
  "extra": {
    "asset-repositories": {
      "bower-asset/polymer": {
        "type": "bower-vcs",
        "url": "https://github.com/Polymer/polymer.git"
      },
      "bower-asset/core-toolbar": {
        "type": "bower-vcs",
        "url": "https://github.com/Polymer/core-toolbar.git"
      }
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "require": {
    "fxp/composer-asset-plugin": "~1.0",
    "bower-asset/core-toolbar": "~0.5.0,<0.6.0",
    "yiisoft/yii2": "~2.0@dev"
  },
  "autoload": {
    "psr-4": {
      "Navarr\\Materialii\\": "src/",
      "Navarr\\Materialii\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  }
}
